{
  "appId": "AspNetCoreAppII",
  "urls": [
    "http://localhost:5000"
  ],
  "router": {
    "default": "index.html",
    "rewrites": [
      {
        "pattern": "^(?!api).*(?<!\\.(html|js|css|woff|jpg|ico|gif|md))$",
        "value": "index.html",
        "mode": "Replace"
      }
    ]
  },
  "res": {
    "mineTypes": {
      "html": "text/html",
      "js": "application/x-javascript",
      "css": "text/css",
      "woff": "application/font-woff",
      "jpg": "image/jpeg",
      "ico": "image/x-icon",
      "gif": "image/gif",
      "md": "text/plain"
    }
  },
  "api": {
    "enableLog": true
  }
}